#include<stdio.h>
int main()
{
	 int maximum, flag, count=0, i, j;
	 
	 scanf("%d", &maximum);
	 
	 for(i=0; i<=maximum; i++)
	 {
		  flag = 0;
		  for(j=2; j <= i/2; j++)
		  {
			   if(i%j==0)
			   {
				    flag=1;
				    break;
			   }
		  }
		  if(flag==0 && i>=2)
		  {
		   	count++;
		  }
	 }
	 printf("%d", count); 
}
